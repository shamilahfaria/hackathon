{
    "manifest_version": 2,
    "name": "CountdownTimer",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["./main.js"],
            "css": ["./styles.css"]
        }
    ],

    "browser_action": {
        "default_icon" : "clock.png",
        "default_title": "CountDown Timer",  // optional, shown in tooltip
        "default_popup": "index.html"
    }
}